# your settings --------------------------------------------
# NOTE: Have always a look to the syntax (|PIN_SIM  = '1234'|)
#       If there not data required lik in "GPRS_USER" mark it with ''.

#PIN from your SIM-Card
|PIN_SIM  = '1234'|

# speed serial port
|COM = '9600'|

# server name
|HOST = '<your server name>'|

# location and script name on your server
|POST = '<conect.php>'|

# APN from your provider
|APN = '<provider APN>'|

# username for GPRS connection (if not used remain empty)
|GPRS_USER  = ''|

# userpassword for GPRS connection (if not used remain empty)
|GPRS_PASS  = ''|

# local time difference to GMT
|GMT = '+1'|

# prefix data string for server application
|PREF = 'data'|

# ######### don't change anything beyond this line #########
|VER = '01-01'|
|CON = 'close'|

